<div id="printEventPdf">
  <table class="table">
    <thead class="thead-dark">
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Description</th>
      <th scope="col">Make Ahead</th>
      <th scope="col">Location</th>
      <th scope="col">Actions</th>
    </tr>
    </thead>
    <tbody>
    <tr>
      <% if current_user.meta.supervisor.events.any? %>
      <tr>
        <% current_user.meta.supervisor.events.each do |e| %>
          <%# e.event_details.each do |d| %>
        <tr>
          <th scope="row"><%= e.name %></th>
          <td> <%= e.description %></td>
          <td> <%= e.isMakeAhead %></td>
          <td><%= e.location.name %></td>
          <!--            <td> <%#= d.capacity_remaining %></td>-->
          <!--            <td> <%#= d.start_time.strftime("%F %H:%M %p") %></td>-->
          <!--            <td><%#= d.end_time.strftime("%F %H:%M %p") %></td>-->
      <% end %>
    <% end %>
    </tr>
    </tbody>
  </table>
</div>