<h1>Upcoming Events</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Location</th>
      <th>Description</th>
    </tr>
  </thead>
  <tbody>
  <% unless @events == nil %>
    <%@events.each do |e| %>
      <tr>
        <td><%=e.name %></td>
        <td><%=e.location %></td>
        <td><%=e.description %></td>
      </tr>
    <% end %>
  <%end %>
  </tbody>
</table>



