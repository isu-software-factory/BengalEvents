<h1>Events Upcoming</h1>
<table>
  <thead>
    <tr>
      <td>Name</td>
      <td>Location</td>
      <td>Description</td>
    </tr>
  </thead>
  <tbody>
  <% unless @events == nil %>
    <%@events.each do |e| %>
      <tr>
        <td><%=e.name %></td>
        <td><%=e.location %></td>
        <td><%=e.description %></td>
      </tr>
    <% end %>
  <%end %>
  </tbody>
</table>



