<h1>Teachers main page</h1>
<h2>Students</h2>
<table>
  <thead>
  <tr>
    <th>Name</th>
    <th>Email</th>
  </tr>
  </thead>
<% @teacher.students.each do |s| %>
    <tr>
      <td><%=s.name %></td>
      <td><%=s.user.email %></td>
    </tr>
  <% end %>
</table>
<%= link_to "Add Students", new_student_path %>