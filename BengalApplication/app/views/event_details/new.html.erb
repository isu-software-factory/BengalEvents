<h2>Input Capacity, Start-time, and End-time for each sessions</h2>
<%#=render  partial: "event_details/form" %>
<%#= form_for([@occasion, @occasion.events.build]) do |f| %>
<%= form_for([@occasion, @event, @event_detail]) do |f| %>

  <p>
    <%= f.label :capacity %>
    <%= f.text_field :capacity %>
  </p>
  <p>
    <%= f.label :start_time %>
    <%= f.text_field :start_time %>
  </p>
  <p>
    <%= f.label :end_time %>
    <%= f.text_field :end_time %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>

<%=link_to "Go Back", occasion_event_path(@occasion,@event) %>